<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.172-ea">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox alignment="CENTER" prefHeight="277.0" prefWidth="600.0">
         <children>
            <Label text="Connect to TCP server" textAlignment="CENTER" wrapText="true">
               <padding>
                  <Insets bottom="30.0" />
               </padding>
               <font>
                  <Font size="22.0" />
               </font></Label>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Host" textAlignment="CENTER" />
                  <Label text="Port" GridPane.rowIndex="1" />
                  <Label text="Login" GridPane.rowIndex="2" />
                  <Label text="Password" GridPane.rowIndex="3" />
                  <TextField fx:id="hostFX" promptText="127.0.0.1" GridPane.columnIndex="1" />
                  <TextField fx:id="portFX" promptText="3552" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="loginFX" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <PasswordField fx:id="passwordFX" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Button mnemonicParsing="false" onAction="#register" text="Register" GridPane.rowIndex="4" />
                  <Button mnemonicParsing="false" onAction="#login" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </children>
         <padding>
            <Insets left="100.0" right="100.0" />
         </padding></VBox>
   </children>
</VBox>
